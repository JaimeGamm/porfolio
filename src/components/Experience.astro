---
import ExperienceItem from "./ExperienceItem.astro"
const EXPERIENCIE = [
  {
    date: "20/06/2023 – 18/12/2023",
    title: "Pasante de Analitica e-Commerce",
    company: " Avianca",
    description:
      "Responsable de la Automatización del Procesos SEO utilizando Python, Estrategia SEO (HREFLANGS y Redirecciones), Pruebas de Sitio Web Manuales y Automatizadas, Elaboración de Informes utilizando Data Studio.",
  }
]
---

<ol class="relative mt-16">
  {
    EXPERIENCIE.map((experiencie) => (
      <li class="">
        <ExperienceItem {...experiencie} />
      </li>
    ))
  }
</ol>
